FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    west
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }

    east
    {
        type            zeroGradient;
    }
    north 
    {
        type            zeroGradient;
    }
    south 
    {
        type            zeroGradient;
    }
    terrain 
    {
        type            zeroGradient;
    }
    top 
    {
        type            zeroGradient;
    }
}

